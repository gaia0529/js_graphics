		link(rel='stylesheet', type='text/css', href='/less/include.css')
		style(type='text/css').
			
		script(src='/js/jquery-1.11.3.min.js')
		script(type='text/javascript').
			var loadImage = function($image, src, callback){
				$image.on('load', function(evt){
					$image.off('load');
					callback($image);
				}).each(function(){
					if($image[0].complete){
						$image.trigger('load');
					}
				});

				if($.browser.webkit){
					$image.attr('src', '');
				}
				$image.attr('src', src);
			}

			$(function(){
				
			});
			
